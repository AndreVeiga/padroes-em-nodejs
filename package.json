{
  "name": "padroes-em-nodejs",
  "version": "1.0.0",
  "description": "Estudo de padrões em NodeJS",
  "main": "index.js",
  "scripts": {
    "test": "mocha ./test/**/*.js",
    "test-report": "nyc --reporter=html mocha ./test/**/*.js",
    "test-report-doc": "mocha ./test/**/*.js --reporter doc > ./test/report/test.html",
    "prestart": "yarn install",
    "start": "node index.js",
    "lint": "eslint --fix --ext .js,.jsx ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndreVeiga/padroes-em-nodejs.git"
  },
  "keywords": [
    "NodeJS",
    "padrões",
    "Patterns",
    "Design"
  ],
  "author": "Andre Machado",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AndreVeiga/padroes-em-nodejs/issues"
  },
  "homepage": "https://github.com/AndreVeiga/padroes-em-nodejs#readme",
  "dependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0"
  },
  "devDependencies": {
    "eslint": "^7.13.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1"
  }
}
